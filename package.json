{
  "name": "trading-simulator",
  "version": "1.0.0",
  "description": "Solana Trading Simulator Server for hosting trading competitions",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest -c e2e/jest.config.js",
    "test:e2e:watch": "jest -c e2e/jest.config.js --watch",
    "test:e2e:runner": "ts-node e2e/run-tests.ts",
    "test:e2e:debug": "ts-node e2e/debug-test.ts",
    "db:init": "ts-node scripts/setup-db.ts",
    "db:check": "ts-node scripts/check-db.ts",
    "db:clean": "NODE_ENV=development ts-node scripts/clean-db.ts",
    "db:reset": "npm run db:clean && npm run db:init",
    "db:migrate": "ts-node src/database/migrate.ts",
    "db:migrate:admin": "ts-node scripts/add-admin-field.ts",
    "db:encrypt-secrets": "ts-node scripts/encrypt-api-secrets.ts",
    "generate:secrets": "ts-node scripts/generate-secrets.ts",
    "setup:admin": "ts-node scripts/setup-admin.ts",
    "setup:all": "ts-node scripts/setup-everything.ts",
    "setup:all:legacy": "npm run generate:secrets && npm run db:init && npm run db:migrate && npm run build && (npm run start & sleep 5 && npm run setup:admin && kill $!)",
    "setup": "npm run generate:secrets && npm run db:init && npm run db:migrate && npm run db:encrypt-secrets",
    "setup:secrets": "npm run generate:secrets && npm run db:encrypt-secrets"
  },
  "keywords": [
    "solana",
    "trading",
    "simulation",
    "competition"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@solana/web3.js": "^1.75.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^18.15.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.10.0",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^4.6.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.14",
    "@types/pg": "^8.6.6",
    "@types/uuid": "^9.0.1",
    "axios": "^1.6.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
